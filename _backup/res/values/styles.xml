        <item name="android:thumb">@null</item>
    </style>
    <style name="volumeSeekBar">
        <item name="android:maxWidth">8.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/seekbar_drawable</item>
        <item name="android:progressDrawable">@drawable/seekbar_drawable</item>
        <item name="android:minWidth">8.0dip</item>
    </style>
    <style name="goagent_SeekBar">
        <item name="android:focusable">true</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/seekbar_drawable</item>
        <item name="android:progressDrawable">@drawable/seekbar_drawable</item>
