    </style>
    <style name="ButtonStyle" parent="@style/TextStyle">
        <item name="android:gravity">center</item>
    </style>
    <style name="ButtonStyle_mdpi" parent="@style/TextStyle_mdpi">
        <item name="android:gravity">center</item>
    </style>
    <style name="ButtonStyle_800_480" parent="@style/TextStyle_800_480">
        <item name="android:gravity">center</item>
    </style>
    <style name="TextStyle">
        <item name="android:textSize">@dimen/main_btn_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextStyle_mdpi">
        <item name="android:textSize">@dimen/main_btn_text_size_mdpi</item>
