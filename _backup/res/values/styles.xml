<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="MediaController_SeekBar">
        <item name="android:maxHeight">16.0dip</item>
        <item name="android:progressDrawable">@drawable/mediacontroller_seekbar</item>
        <item name="android:minHeight">16.0dip</item>
        <item name="android:thumb">@drawable/mediacontroller_seekbar_thumb</item>
    </style>
    <style name="MediaController_Text">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="customer_progress_dialog" parent="@android:style/Widget.ProgressBar">
        <item name="android:fullDark">@drawable/uibase_dialog_bg</item>
        <item name="android:topDark">@drawable/uibase_dialog_bg</item>
        <item name="android:centerDark">@drawable/uibase_dialog_bg</item>
        <item name="android:bottomDark">@drawable/uibase_dialog_bg</item>
        <item name="android:fullBright">@drawable/uibase_dialog_bg</item>
        <item name="android:topBright">@drawable/uibase_dialog_bg</item>
        <item name="android:centerBright">@drawable/uibase_dialog_bg</item>
        <item name="android:bottomBright">@drawable/uibase_dialog_bg</item>
        <item name="android:bottomMedium">@drawable/uibase_dialog_bg</item>
        <item name="android:centerMedium">@drawable/uibase_dialog_bg</item>
        <item name="android:indeterminateDuration">2000</item>
    </style>
    <style name="theme.customer_progress_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:alertDialogStyle">@style/customer_progress_dialog</item>
        <item name="android:progressBarStyleSmall">@style/customer_progress_dialog</item>
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="download_SeekBar">
        <item name="android:focusable">true</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/progressbar_background</item>
        <item name="android:progressDrawable">@drawable/progressbar_background</item>
        <item name="android:thumb">@null</item>
        <item name="android:thumbOffset">4.0dip</item>
    </style>
    <style name="Theme" parent="@android:style/Theme">
        <item name="android:screenOrientation">landscape</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="Animation">
        <item name="android:screenOrientation">landscape</item>
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>
    <style name="VideoTheme">
        <item name="android:screenOrientation">landscape</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/Animation</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="playerSeekBar">
        <item name="android:maxHeight">9.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/seekbar_drawable</item>
        <item name="android:progressDrawable">@drawable/seekbar_drawable</item>
        <item name="android:minHeight">9.0dip</item>
    </style>
    <style name="playerSeekBar_mdpi">
        <item name="android:maxHeight">13.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/seekbar_drawable</item>
        <item name="android:progressDrawable">@drawable/seekbar_drawable</item>
        <item name="android:minHeight">13.0dip</item>
        <item name="android:thumb">@null</item>
